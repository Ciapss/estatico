<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{#if title}}{{title}} â€“ {{/if}}{{project}}</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<link rel="stylesheet" href="/assets/css/main.css">
	<link rel="stylesheet" href="/assets/css/styleguide.css">
</head>
<body>
	<div class="sg_wrapper">

		<nav class="sg_nav">
			<h1 class="sg_title">{{project}} styleguide</h1>

			<h2>Pages</h2>

			<ul>
			{{#each pages}}
				<li>
					<a href="{{this.previewUrl}}" target="content">{{this.title}}</a>
				</li>
			{{/each}}
			</ul>

			<h2>Modules</h2>

			<ul>
			{{#each modules}}
				<li>
					<a href="{{this.previewUrl}}" target="content">
						{{this.title}}
						{{#if jira}}({{this.jira}}){{/if}}
					</a>
				</li>
			{{/each}}
			</ul>

			<h2>Styleguide</h2>

			<ul>
			{{#each styleguide}}
				<li>
					<a href="{{this.previewUrl}}" target="content">{{this.title}}</a>
				</li>
			{{/each}}
			</ul>
		</nav>
		<div class="sg_main">
			<iframe name="content"></iframe>
		</div>

	</div>
</body>
</html>
